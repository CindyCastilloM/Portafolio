<%- include ('template/t_cabecera', {tituloweb: 'Empleados' }); %>
  <div class="container">
    <center>
      <h3>Bienvenido</h3>
      <h3>Zona</h3>
    </center>
    <h1>Empleados</h1><br>
    <a class="btn btn-success btn-block margin-4" href="/empleado/crear">Crear un nuevo empleado </a>
    <% if (arrayempleado.length> 0) { %>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Centro de Trabajo</th>
            <th scope="col">Proceso</th>
            <th scope="col">Clave de Jefatura</th>
            <th scope="col">Jefatura</th>
            <th scope="col">Jefe</th>
            <th scope="col">RPE</th>
            <th scope="col">Nivel de Autoridad</th>
            <th scope="col">Correo</th>
            <th scope="col">Acci√≥n</th>
          </tr>
        </thead>
        <tbody>
          <% arrayempleado.forEach(elementoArray=> { %>
            <tr>
              <td>
                <%= elementoArray.centrot %>
              </td>
              <td>
                <%= elementoArray.proceso %>
              </td>
              <td><%= elementoArray.cvjefatura %></td>
              <td><%= elementoArray.jefatura %></td>
              <td><%= elementoArray.jefe %></td>
              <td><%= elementoArray.rpe %></td>
              <td><%= elementoArray.nlvautoridad %></td>
              <td><%= elementoArray.correo %></td>
              <td>
                
                <a class="btn btn-warning btn-sm" href="empleado/<%= elementoArray.id %>">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-pen"
                    viewBox="0 0 16 16">
                    <path
                      d="m13.498.795.149-.149a1.207 1.207 0 1 1 1.707 1.708l-.149.148a1.5 1.5 0 0 1-.059 2.059L4.854 14.854a.5.5 0 0 1-.233.131l-4 1a.5.5 0 0 1-.606-.606l1-4a.5.5 0 0 1 .131-.232l9.642-9.642a.5.5 0 0 0-.642.056L6.854 4.854a.5.5 0 1 1-.708-.708L9.44.854A1.5 1.5 0 0 1 11.5.796a1.5 1.5 0 0 1 1.998-.001zm-.644.766a.5.5 0 0 0-.707 0L1.95 11.756l-.764 3.057 3.057-.764L14.44 3.854a.5.5 0 0 0 0-.708l-1.585-1.585z" />
                  </svg>Editar
                </a>

              </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
      <% } %>
   
  </div>

  

